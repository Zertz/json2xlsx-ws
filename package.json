{
  "name": "json2xlsx-ws",
  "version": "0.1.0",
  "description": "Convert JSON objects to a xlsx worksheet",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "compile": "babel src -d lib",
    "prepublish": "npm test",
    "pretest": "npm run lint && npm run compile",
    "test": "mocha --compilers js:babel-core/register -R spec ./test"
  },
  "peerDependencies": {
    "xlsx": "^0.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.0",
    "mocha": "^2.3.0",
    "standard": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zertz/xlsx-json2ws.git"
  },
  "keywords": [
    "convert",
    "json",
    "object",
    "xlsx",
    "worksheet"
  ],
  "author": "Pier-Luc Gendreau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zertz/xlsx-json2ws/issues"
  },
  "homepage": "https://github.com/Zertz/xlsx-json2ws#readme",
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "standard": {
    "ignore": [
      "lib"
    ],
    "globals": [
      "describe",
      "it"
    ]
  }
}
